FUNCTION mod :
PARAM v1
PARAM v2
t2 := v1
t6 := v1
t7 := v2
t4 := t6 / t7
t5 := v2
t3 := t4 * t5
t1 := t2 - t3
RETURN t1
FUNCTION DigitSum :
PARAM v3
t8 := v3
t9 := #0
IF t8 == t9 GOTO label1
GOTO label2
LABEL label1 :
t10 := #0
RETURN t10
LABEL label2 :
ARG #10
ARG v3
t12 := CALL mod
t15 := v3
t16 := #10
t14 := t15 / t16
ARG t14
t13 := CALL DigitSum
t11 := t12 + t13
RETURN t11
FUNCTION main :
READ t18
&v4 := t18
t17 := &v4
t19 := &v4
t20 := #0
IF t19 < t20 GOTO label3
GOTO label4
LABEL label3 :
t22 := #1
t21 := #0 - t22
WRITE t21
GOTO label5
LABEL label4 :
ARG &v4
t23 := CALL DigitSum
WRITE t23
LABEL label5 :
t24 := #0
RETURN t24
